{% extends '::layout_admin.html.twig' %}

{% block body -%}
    <h1>Common\Destination</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Knownas</th>
                <td>{{ entity.knownAs }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
            <tr>
                <th>Lft</th>
                <td>{{ entity.lft }}</td>
            </tr>
            <tr>
                <th>Lvl</th>
                <td>{{ entity.lvl }}</td>
            </tr>
            <tr>
                <th>Rgt</th>
                <td>{{ entity.rgt }}</td>
            </tr>
            <tr>
                <th>Root</th>
                <td>{{ entity.root }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_destination') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_destination_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>

</ul>
{% endblock %}
